                                <xs:attribute name="reference_id" type="xs:string" />
                                <xs:attribute name="fee" type="xs:string" />
                                <xs:attribute default="Y" name="is_borrower" type="base_YN">
                                  <xs:annotation>
                                    <xs:documentation>Used to indicate if card holder is an authorized user (N) or just an applicant on the application (Y).</xs:documentation>
                                  </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="suffix">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:maxLength value="20" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:attribute>
                                <xs:attribute name="credit_limit" type="base_money" />
                                <xs:attribute name="ssn" type="xs:string" />
                                <xs:attribute name="dob" type="xs:string" />
                                <xs:attribute name="mother_maiden" type="xs:string" />
                                <xs:attribute name="card_account_number" type="xs:string" />
                                <xs:attribute name="is_ofac_istwatch">
                                  <xs:simpleType>
                                    <xs:restriction base="base_YN">
                                      <xs:maxLength value="50" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:attribute>
                                <xs:attribute name="istwatch_transactiondate">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:maxLength value="50" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:attribute>
                                <xs:attribute name="new_customer_setup" type="base_YN" />
                                <xs:attribute name="has_monthly_statement" type="base_YN" />
                                <xs:attribute name="is_guarantor" type="base_YN" />
                                <xs:attribute name="employee_id" type="xs:string" />
                                <xs:attribute name="card_holder_name_on_card" type="xs:string" />
                                <xs:attribute name="mailing_address" type="xs:string" />
                                <xs:attribute name="mailing_city" type="xs:string" />
                                <xs:attribute name="mailing_zip" type="xs:string" />
                                <xs:attribute name="mailing_state" type="base_state" />
                                <xs:attribute name="shipping_method" type="xs:string" />
                                <xs:attribute name="home_phone" type="xs:string" />
                                <xs:attribute name="work_phone" type="xs:string" />
                                <xs:attribute name="control_account_number" type="xs:string" />
                                <xs:attribute name="company_name" type="xs:string" />
                                <xs:attribute name="company_name_on_card" type="xs:string" />
                                <xs:attribute name="statement_address" type="xs:string" />
                                <xs:attribute name="statement_city" type="xs:string" />
                                <xs:attribute name="statement_zip" type="xs:string" />
                                <xs:attribute name="statement_state" type="base_state" />
                                <xs:attribute name="cash_limit" type="base_money" />
                                <xs:attribute name="annual_fee_code" type="xs:string" />
                                <xs:attribute name="paypoint_flag" type="xs:string" />
                                <xs:attribute name="has_ghost_card_letter" type="base_YN" />
                                <xs:attribute name="authorization_strategy" type="xs:string" />
                                <xs:attribute name="custom_auth_strategy" type="base_YN" />
                                <xs:attribute name="fleet_restriction" type="xs:string" />
                                <xs:attribute name="fleet_prompt" type="xs:string" />
                                <xs:attribute name="fleet_group" type="xs:string" />
                                <xs:attribute name="fleet_type" type="xs:string" />
                                <xs:anyAttribute namespace="http://www.meridianlink.com/InternalUse">
                                  <xs:annotation>
                                    <xs:documentation>This field is internal_card_holder_id for internal use only. LoansPQ will export this value and will import it, but never add this yourself or pull from another application. This internal value is auto generated by LPQ when creating a new card holder.</xs:documentation>
                                  </xs:annotation>
                                </xs:anyAttribute>
                              </xs:complexType>
                            </xs:element>
                          </xs:sequence>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" name="VEHICLE_INFO">
