                <xs:sequence>
                  <xs:element name="VEHICLES">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element maxOccurs="unbounded" name="VEHICLE" type="VEHICLE_FINANCING" />
                        <xs:element minOccurs="0" name="TRADE_IN" type="VEHICLE_TRADE_IN" />
