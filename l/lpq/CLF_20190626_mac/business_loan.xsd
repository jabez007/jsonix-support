          <xs:element minOccurs="0" name="CONTACTS" type="BASE_LOAN_CONTACTS" />
          <xs:element maxOccurs="unbounded" name="SYSTEM" type="BUSINESS_SYSTEM" />
          <xs:element minOccurs="0" name="INTEGRATION_DATA_LIST">
            <xs:complexType>
