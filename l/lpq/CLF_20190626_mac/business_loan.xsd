                    <xs:attribute name="business_id_type" type="base_business_id_type" />
                    <xs:attribute name="business_id_number">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:maxLength value="40" />
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="business_id_state">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:maxLength value="2" />
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="business_id_country">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:maxLength value="20" />
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="business_id_expire_date" type="xs:date" />
                    <xs:attribute name="business_id_date_issued" type="xs:date" />
                    <xs:attribute name="business_id_verification_date" type="xs:date" />
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="BUSINESS_GUARANTORS">
